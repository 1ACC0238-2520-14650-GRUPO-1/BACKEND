{
  "version": 2,
  "builds": [
    {
      "src": "app/main.py",
      "use": "@vercel/python@^4.3.1"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "app/main.py"
    },
    {
      "src": "/(.*)",
      "dest": "app/main.py"
    }
  ],
  "env": {
    "PYTHONUNBUFFERED": "1"
  }
}
